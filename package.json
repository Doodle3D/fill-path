{
  "name": "fill-path",
  "version": "0.0.1",
  "description": "Vector based fill tool (like floodfill)",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:serve",
    "test:serve": "npm run serve -- --startPath='tests/'",
    "serve": "browser-sync start --server --files='**, !jspm_packages, !node_modules'"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/Doodle3D/fill-path.git"
  },
  "keywords": [
    "vector",
    "fill",
    "bucket",
    "clipper",
    "doodle3d",
    "path",
    "floodfill"
  ],
  "author": "Casper @ Doodle3D",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.11.1",
    "chokidar-socket-emitter": "^0.5.0",
    "jspm": "^0.16.29"
  },
  "format": "esm",
  "directories": {
    "lib": "lib"
  },
  "jspm": {
    "directories": {
      "lib": "lib"
    },
    "dependencies": {
      "clipper-lib": "npm:clipper-lib@^1.0.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4",
      "tape": "npm:tape@^4.4.0"
    }
  }
}
